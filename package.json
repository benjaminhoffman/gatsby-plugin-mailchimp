    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^5.16.0",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^2.1.0",
        "webpack": "^4.29.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    }
